# Google OAuth 설정 (실제 키는 .env 파일에 저장하세요)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# 데이터베이스 설정
DATABASE_URL=your_database_url_here

# JWT 토큰 시크릿 
JWT_SECRET=your_jwt_secret_here

# 세션 시크릿 
SESSION_SECRET=your_session_secret_here

# Vite 프론트엔드용 구글 맵스 API 키
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here

# AI 모델 선택 (비용 절감용)
# 전체 서비스에 적용할 모델 (기본값: gpt-5.1-chat-latest)
# 옵션: gpt-5.1-chat-latest, gpt-5-mini, gpt-4.1, gpt-4o 등
AI_MODEL=gpt-5.1-chat-latest

# 서비스별 개별 모델 설정 (선택사항 - 미설정시 AI_MODEL 사용)
# CINEMAP_AI_MODEL=gpt-5.1-chat-latest
# MINI_CONCIERGE_AI_MODEL=gpt-5-mini
# CONCIERGE_AI_MODEL=gpt-5-mini

# Google Translate API (DM 번역용)
GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here